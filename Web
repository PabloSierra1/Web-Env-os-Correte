      <div class="container" data-aos="fade-up">
        <p></p>
        <div id="salida2"></div>
      </div>

        <script>
          function Calcula(origen1, destino1, peso1) {
              let resultado = ''
              console.log("origen", origen1)
      
              if (origen1 === "palmauno" && destino1 === "palmados") resultado = 5
              if (origen1 === "palmauno" && destino1 !== "palmados") resultado = 9
              if (origen1 !== "palmauno" && destino1 === "palmados") resultado = 9
              if (origen1 !== "palmauno" && destino1 !== "palmados") resultado = 7
      
              if (peso1>10 && peso1<20) resultado = resultado *1.5
              if (peso1>20) resultado = resultado *2
              if (peso1 === "") resultado = 0
      
              
              salida.innerHTML = `<h3>El precio son ${resultado} € `
          }

        </script>

        <script>
          function Calculados(origen2, destino2, peso2) {
              let resultado = ''

              if (origen2 === destino2) resultado = 7
              if (origen2 !== destino2) resultado = 7
              if (origen2 === "" && destino2 === "") resultado = 0

              if (peso2>10 && peso2<20) resultado = resultado *1.5
              if (peso2>20) resultado = resultado *2
              if (peso2 === "") resultado = 0

      
              salida2.innerHTML = `<h3>El precio son ${resultado} € `
          }

        </script>


        
    

